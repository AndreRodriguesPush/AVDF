@*@using System.Web.Optmization*@
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>UFMT - Gerenciamento de Processo de Bolsas</title>

    <!-- Bootstrap -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" type="text/css">

    <link href="~/Content/css/estilo.css" rel="stylesheet" type="text/css">
    <link href="~/Content/css/estilo-menu-grafica.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/jquery-ui.css" rel="stylesheet" type="text/css">

    @*@if (Session["altocontraste"] != null)
        {
            <link href="~/Content/css/estilo-contraste.css" rel="stylesheet" type="text/css">
        }*@
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script type="text/javascript" src="~/Scripts/jquery-3.1.1.min.js"></script>

    @*<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>*@

    @if (ViewBag.Mobile != null && ViewBag.Mobile)
    {
        <style>
            #head {
                width: 100%;
                padding-left: 10px;
            }

            #sect {
                width: 100% !important;
            }
        </style>
    }
</head>
<body>
    <div id="barra-brasil">
        <a href="http://brasil.gov.br" style="background:#7F7F7F;  padding:4px 0 4px 10px; display: block; font-family:sans,sans-serif; text-decoration:none; color:white; ">Portal do Governo Brasileiro</a>
    </div>
    <div id="gradient">
        <header id="head-align">
            <div id="head">
                <div class="acessibilidade-espaco">
                    @*@if (Session["altocontraste"] != null)
                        {
                            <a href="@Url.Action("AltoContraste", "Home")" id="btnAltoContraste" class="acessibilidade" title="Desativar Alto Contraste">CONTRASTE PADRÃO</a>

                        }
                        else
                        {
                            <a href="@Url.Action("AltoContraste", "Home")" id="btnAltoContraste" class="acessibilidade" title="Ativar Alto Contraste">ALTO CONTRASTE</a>
                        }*@

                    <a href="#" id="btnDiminuir" class="acessibilidade" title="Diminuir Fonte">A-</a>
                    <a href="#" id="btnAumentar" class="acessibilidade" title="Aumentar Fonte">A+</a>
                </div>
                <div class="ancoras">
                    <a href="#container" id="irparaconteudo" class="" title="Ir para o Conteúdo">Ir para o Conteúdo <span>1</span></a>
                    <a href="#categorias" id="irparamenu" class="" title="Ir para o Menu">Ir para o Menu <span>2</span></a>
                    <a href="#buscar-campo" id="irparabusca" class="" title="Ir para a busca">Ir para a busca <span>3</span></a>
                    <a href="#footer" id="irpararodape" class="" title="Ir para o rodapé">Ir para o rodapé <span>4</span></a>
                </div>
                <div id="logo">
                    <div class="logomg">
                    </div>
                    @*@if (ViewBag.Mobile != null && ViewBag.Mobile)
                        {
                            <img src="@Url.Content("~/Content/img/logo_ufmt_nome.png")" />
                        }
                        else
                        {
                            <img src="@Url.Content("~/Content/img/logo_ufmt_sem_selo.png")" />

                        }*@
                </div>
                <div id="logotexto">
                    <span>Universidade Federal de Mato Grosso</span>
                    <h1>Sistema de Gerencial de Bolsas</h1>
                </div>
            </div>
        </header>
    </div>
    <section id="sect-align">
        <!-- div de alinhamento -->
        <nav id="nav">
            <div class="InfoItens">
                <ul>
                    <!-- outros items LI podem ser inseridos aqui <li><a href="#">Informações</a></li> <li class="InfoIcon"><a href="#">Informações de Acesso</a></li>-->
                    <li><a href="https://www.ufmt.br"><span class="glyphicon glyphicon-home"></span>&nbsp; Portal da UFMT</a></li>
                </ul>
            </div>
        </nav>
    </section>
    <section id="sect">

        <div id="container">
            <div class="row">
                <div class="col-xs-12"><h1></h1></div>
            </div>

            <div class="row">

                <div class="col-sm-12">

                    @RenderBody()

                </div>

            </div>
        </div>
    </section>
    <!-- acaba div do conteúdo principal -->
    <!-- div de alinhamento -->
    <div class="footerAnimado">
        <img src="~/Content/img/rodape_animado.gif" alt="Imagem animada, viola de coxo e Tuiuiú" />
    </div>
    <footer id="footer-align">
        <div id="footer">
            <p>
                Universidade Federal de Mato Grosso - UFMT<br>
                Secretaria de Tecnologia da Informação - STI<br>
                Av. Fernando Correa da Costa, nº 2367 - Bairro Boa Esperança. Cuiabá - MT - 78060-900
            </p>

            <p>Fone: +55 (65) 3615-8028</p>
            <p>Contato: <a href="mailto:ces@ufmt.br">ces@ufmt.br</a></p>
        </div>
    </footer>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script async type="text/javascript" src="~/Scripts/bootstrap.js"></script>
    <script type="text/javascript" src="~/Scripts/angular.min.js"></script>
    <script type="text/javascript" src="~/Scripts/app.js"></script>
    <script async type="text/javascript" src="https://barra.brasil.gov.br/barra.js"></script>
    <script type="text/javascript" src="~/Scripts/acessibilidade.js"></script>

    <!--Include Scripts para o plugin DataTable Jquery-->
    <link href="@Url.Content("~/Content/DataTables/css/dataTables.bootstrap.css")" rel="stylesheet" />

    <script src="~/Scripts/DataTables/jquery.dataTables.js"></script>
    <script src="~/Scripts/DataTables/dataTables.bootstrap.js"></script>

    <script src="@Url.Content("~/Scripts/globalize/globalize.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/globalize/cultures/globalize.culture.pt-BR.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/jquery.maskMoney.min.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/jquery-ui.js")" type="text/javascript"></script>
    @RenderSection("Scripts", false)

</body>
</html>
